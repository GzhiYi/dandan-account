.list-page {
  background-color: #f8fafb;
}
.content {
  position: relative;
  /* width: 100%; */
  height: 88rpx;
  display:flex;
  align-items:center;
  padding:0 0 0 18rpx;
}
/* 新增图标 */
.nav-icon-view {
  border-radius:50%;
  width:70rpx;
  height:70rpx;
  display:flex;
  justify-content:center;
  align-items:center;
  margin-right: 22rpx;
}
.nav-icon-view-back {
  width:70rpx;
  height:70rpx;
  display:flex;
  justify-content:center;
  align-items:center;
  margin-right: 22rpx;
}
.has-font {
  width:74px;
  border-radius:50rpx;
  font-size:33rpx;
  padding:0 10rpx;
}
.nav-icon {
  width: 42rpx;
  height: 42rpx;
}
.loading-icon {
  animation: headRotate 2s linear infinite;
}
@keyframes headRotate{
  0% {transform: rotate(0deg);}
  50% {transform: rotate(180deg);}
  100% {transform: rotate(360deg);}
}